<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xmlns:context="http://www.springframework.org/schema/context"
		xsi:schemaLocation="http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context 
		http://www.springframework.org/schema/context/spring-context.xsd">
		
		<bean id="person" class="com.javawiz.basic.xml.autowire.bytype.Person" autowire="byType">
			<property name="name" value="Ranjan"/>
		</bean>
		
		<bean id="address" class="com.javawiz.basic.xml.autowire.bytype.Address">
			<property name="city" value="Delhi"/>
			<property name="state" value="UterPradesh"/>
			<property name="country" value="India"/>
		</bean>
		
</beans>